<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="323.0" prefWidth="394.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9.0.1" fx:controller="application.AveDevController">
   <children>
      <Pane prefHeight="323.0" prefWidth="394.0">
         <children>
            <ToolBar prefHeight="40.0" prefWidth="394.0">
              <items>
                <Button mnemonicParsing="false" onAction="#openAction" text="Open..." />
                  <Button layoutX="10.0" layoutY="13.0" mnemonicParsing="false" onAction="#execAction" text="Exec..." />
                  <Button layoutX="67.0" layoutY="13.0" mnemonicParsing="false" onAction="#saveAction" text="Save..." />
                  <Button mnemonicParsing="false" onAction="#saveLogAction" text="Save log..." />
                  <Button layoutX="119.0" layoutY="13.0" mnemonicParsing="false" onAction="#quitAction" text="Quit" />
              </items>
            </ToolBar>
            <ToolBar layoutY="40.0" prefHeight="40.0" prefWidth="394.0">
              <items>
                  <Label text="Eliminate char">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <TextField fx:id="eliminateField" />
              </items>
            </ToolBar>
            <TextArea fx:id="log" layoutY="80.0" prefHeight="245.0" prefWidth="394.0" />
         </children>
      </Pane>
   </children>
</AnchorPane>
